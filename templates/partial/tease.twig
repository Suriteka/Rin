{% block content %}
	<article class="tease">
		<h2 class="tease__title"><a href="{{ post.link }}">{{ post.title }}</a></h2>
		<date class="tease__date">{{ post.date }}</date>
		<p class="tease__preview">{{ post.preview.read_more('') }}</p>
		<span class="tease__button"">Lire la suite</span>
	</article>
{% endblock %}
